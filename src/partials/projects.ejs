<!-- List of personal projects -->

<div class="section">
    <h5>Projects</h5>
    <br>
    <% projects.forEach((project) => { %>
        <div class="project-box">
            <h6><a href="<%= project.repo %>"><%= project.name %></a></h6>
            <p><%= project.description %></p>
            <p>
                <%if (project.tools.length > 0) { %>
                    <strong>Tools: </strong>
                <% } %>
                <%= project.tools.split(",").join(" â€¢ ") %>
            </p>
        </div>
    <% }) %>
</div>